{
  "name": "prisma-client-reason",
  "namespace": false,
  "jsx": {
    "version": 4
  },
  "bs-dependencies": [],
  "bs-dev-dependencies": [
    "@glennsl/rescript-jest"
  ],
  "ppx-flags": [],
  "sources": [
    {
      "dir": "src",
      "subdirs": false
    },
    {
      "dir": "__tests__",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".mjs",
  "warnings": {
    "number": "-3",
    "error": "+101+8"
  },
  "gentypeconfig": {
    "language": "typescript",
    "exportInterfaces": true,
    "shims": {},
    "importPath": "relative",
    "generatedFileExtension": ".gen.ts",
    "module": "es6",
    "debug": {
      "all": false,
      "basic": false
    }
  }
}

{
  "name": "prisma-client-reason",
  "version": "0.0.1",
  "author": "Torben Ewert",
  "type": "module",
  "contributors": [
    {
      "name": "Rob Hilgefort",
      "email": "rjhilgefort@gmail.com",
      "url": "http://rob.hilgefort.me/"
    }
  ],
  "license": "MIT",
  "main": "dist/generator.js",
  "scripts": {
    "clean": "run-p clean:*",
    "clean:node_modules": "rm -rf node_modules",
    "clean:build": "rm -rf dist",
    "start": "nodemon",
    "build": "rescript && tsc && chmod +x ./dist/generator.js",
    "rebuild": "run-s clean:build build",
    "res:start": "rescript build -w",
    "resetup": "run-s clean setup",
    "test": "rescript && NODE_OPTIONS=--experimental-vm-modules jest",
    "test:start": "NODE_OPTIONS=--experimental-vm-modules jest --watchAll"
  },
  "dependencies": {
    "@prisma/generator-helper": "^4.11.0",
    "common-tags": "^1.8.2",
    "fs-extra": "^11.1.0",
    "lodash": "^4.17.21",
    "reason-promise": "^1.1.5"
  },
  "devDependencies": {
    "@glennsl/rescript-jest": "^0.9.2",
    "@swc/core": "^1.3.39",
    "@swc/jest": "^0.2.24",
    "@types/common-tags": "^1.8.1",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "*",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "npm-check-updates": "^16.7.12",
    "npm-run-all": "^4.1.5",
    "rescript": "~10",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5"
  },
  "nodemonConfig": {
    "exec": "tsc && yalc push",
    "ext": "ts"
  }
}
